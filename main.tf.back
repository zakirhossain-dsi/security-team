data "terraform_remote_state" "network" {
  backend = "remote"
  config = {
    organization = "rbts"
    workspaces = {
      name = "network-team"
    }
  }
}

output "network_public_ips" {
  value = data.tfe_outputs.network.outputs.public_ips
  description = "List of public IPs fetched from the Network workspace"
}